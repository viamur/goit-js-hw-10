{{#each this}}
  <div>
    <img src={{flags.svg}} alt={{name.official}} style='width: 25px;margin-right: 10px;' />
    <h2>{{name.official}}</h2>
  </div>
  <p><span>Capital:</span>{{capital}}</p>
  <p><span>Population:</span>{{population}}</p>
  <p><span>Languages:</span>{{#each languages}} {{this}}, {{/each}}</p>
{{/each}}

